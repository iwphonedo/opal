{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Debug with Args",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "args": [
        "--deploy",
        "--with_broadcast",
      ],
      "console": "integratedTerminal"
    }
  ]
}